<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ambcab">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Maps_activity"></activity>
        <activity android:name=".ambulanceRegistration" />
        <activity android:name=".welcomePage" />
        <activity android:name=".BottomNavigation"/>
        <activity android:name=".patient_Homepage" />
        <activity android:name=".MedicalDetails" />
        <activity android:name=".PersonalDetails" />
        <activity android:name=".Select_user" />
        <activity android:name=".verifyotp" />
        <activity android:name=".register" />
        <activity android:name=".starting" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/maps_key" />
    </application>

</manifest>